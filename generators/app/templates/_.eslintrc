{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "globals": {
        "sap": true,
        "jQuery": true
    },
    "rules": {
        "max-lines": [
            "error",
            {
                "max": 500,
                "skipBlankLines": true,
                "skipComments": true
            }
        ],
        "prefer-const": "error",
        "no-var": "error",
        "no-unused-vars": "error",
        "no-multi-assign": "error",
        "no-plusplus": "error",
        "no-undef": "error",
        "one-var": ["error", "never"],
        "no-new-object": "error",
        "object-shorthand": ["error", "always"],
        "no-prototype-builtins": "error",
        "prefer-object-spread": "error",
        "no-array-constructor": "error",
        "array-callback-return": "error",
        "prefer-template": "error",
        "template-curly-spacing": "error",
        "no-eval": "error",
        "no-loop-func": "error",
        "default-param-last": "error",
        "no-new-func": "error",
        "arrow-spacing": "error",
        "no-confusing-arrow": "error",
        "dot-notation": "error",
        "no-mixed-operators": "error",
        "eqeqeq": "error",
        "brace-style": "error",
        "no-else-return": "error",
        "spaced-comment": "error",
        "keyword-spacing": "error",
        "space-unary-ops": "error",
        "no-trailing-spaces": [
            "error",
            {
                "ignoreComments": true
            }
        ],
        "key-spacing": [
            "warn",
            {
                "beforeColon": false
            }
        ],
        "comma-spacing": [
            "warn",
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": "error",
        "semi": "error",
        "radix": "off",
        "no-new-wrappers": "error",
        "id-length": "off",
        "new-cap": "warn",
        "camelcase": ["warn"],
        "consistent-return": "warn",
        "no-warning-comments": "warn",
        "default-case": "warn",
        "no-console": "error",
        "no-debugger": "error",
        "no-shadow": "error",
        "no-irregular-whitespace": "error"
    },
    "plugins": ["@sap/ui5-jsdocs"],
    "extends": ["plugin:@sap/ui5-jsdocs/recommended", "eslint:recommended"]
}
